<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/model_nav_graph"
    app:startDestination="@id/modelFoundationFragment">
    <fragment
        android:id="@+id/modelFoundationFragment"
        android:name="training20.tcmobile.fragments.ModelFoundationFragment"
        android:label="HairdresserFoundationFragment">
        <action
            android:id="@+id/action_modelFoundationFragment_to_modelMenuSearchFragment"
            app:destination="@id/modelMenuSearchFragment" />
        <action
            android:id="@+id/action_modelFoundationFragment_to_modelMenuFragment"
            app:destination="@id/modelMenuFragment" />
        <action
            android:id="@+id/action_modelFoundationFragment_to_modelChatRoomFragment"
            app:destination="@id/modelChatRoomFragment" />
    </fragment>
    <fragment
        android:id="@+id/modelMenuSearchFragment"
        android:name="training20.tcmobile.fragments.ModelMenuSearchFragment"
        android:label="ModelMenuSearchFragment" />
    <fragment
        android:id="@+id/modelMenuFragment"
        android:name="training20.tcmobile.fragments.ModelMenuFragment"
        android:label="ModelMenuFragment" >
        <action
            android:id="@+id/action_modelMenuFragment_to_modelChatRoomFragment"
            app:destination="@id/modelChatRoomFragment" />
    </fragment>
    <fragment
        android:id="@+id/modelChatRoomFragment"
        android:name="training20.tcmobile.fragments.ModelChatRoomFragment"
        android:label="ModelChatRoomFragment">
        <argument
            android:name="chatRoomId"
            app:argType="integer" />
        <argument
            android:name="hairdresser"
            app:argType="training20.tcmobile.mvvm.models.Hairdresser"
            />
    </fragment>
</navigation>